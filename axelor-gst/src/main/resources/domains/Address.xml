<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.apps.gst.db" />

	<entity name="Address_Gst" lang="java">
		<many-to-one name="address"
			ref="com.axelor.apps.base.db.Address" title="Address"
			initParam="true" massUpdate="true" />
		<many-to-one name="state" title="State"
			ref="com.axelor.apps.gst.db.State" />
	</entity>

</domain-models>
